Public Sub llenar_grid(ByVal Grilla As MSHFlexGrid)
Dim nsaldo As Double
Dim in_facturado As Double
Dim in_acumulado_total As Double
Dim in_operador As String
On Error GoTo salir
Call ConfiguraRst(strCadena)
If rst.RecordCount < 1 Then
    Grilla.Rows = 0
    Me.cmdreporte.Enabled = False
    Me.cmdReportedetallado.Enabled = False
    Me.cmdamortizar.Enabled = False
    Me.cmdGeneral.Enabled = False
    Exit Sub
End If
   Me.cmdreporte.Enabled = True
   Me.cmdReportedetallado.Enabled = True
   Me.cmdGeneral.Enabled = True
    Grilla.Rows = 0
   
   ReDim arrColWidth(1 To rst.Fields.Count)
       For Each Campo In rst.Fields
           Grilla.ColWidth(0) = 0
           Grilla.ColWidth(1) = 1000
           Grilla.ColWidth(2) = 1000
           Grilla.ColWidth(3) = 2200
           Grilla.ColWidth(4) = 1200
           Grilla.ColWidth(5) = 3000
           Grilla.ColWidth(6) = 900
           Grilla.ColWidth(7) = 700
           Grilla.ColWidth(8) = 1300
           Grilla.ColWidth(9) = 1300
           Grilla.ColWidth(10) = 1300
           Grilla.ColWidth(11) = 2000
           Grilla.ColWidth(12) = 2200
           
        Next
         
         cabecera = "IDVENTA" & vbTab & "EMISION" & vbTab & "VENCIMIENTO" & vbTab & "COMPROBANTE" & vbTab & "DNI CLIENTE" & vbTab & "DATOS CLIENTE" & vbTab & "MONEDA" & vbTab & "TC" & vbTab & "  TOTAL   " & vbTab & "SALDO [DOLAR]" & vbTab & "SALDO [SOLES] " & vbTab & "OPERADOR" & vbTab & "REFERENCIA" & vbTab & "%SEGURO"
         Grilla.AddItem cabecera
         
         For k = 1 To 12
            Grilla.col = k
            Grilla.Row = 0
            Grilla.CellBackColor = &HDFDFE0
         Next k
         
        rst.MoveFirst
        nfactor = 0
        in_acumulado = 0
        in_acumulado_total = 0
        For i = 0 To rst.RecordCount - 1
             in_tseguro = rst("tseguro") * rst("factor") / 100
             If IsNull(rst("guia")) = True Then
                in_operador = Mid(UCase(rst("nombre_completo")), 1, 20)
             Else
                in_operador = rst("guia") & Space(4) & "[ S/." & rst("tseguro") & " ]"
             End If
             
            
                If rst("id_moneda") = "00002" Then
                   in_facturado = rst("total") * rst("tc")
                    n_saldo_dolar = (rst("total") - rst("pago"))
                    n_saldo = (rst("total") - rst("pago")) * rst("tc")
                Else
                    n_saldo = (rst("total") - rst("pago"))
                    n_saldo_dolar = (rst("total") - rst("pago")) / rst("tc")
                    in_facturado = rst("total")
                End If
                        
                in_acumulado_total = in_acumulado_total + in_facturado
          
             
             
             
             Fila = rst("id_venta") & vbTab & Format(rst("fecha_emision"), "dd-mm-YYYY") & vbTab & Format(rst("fecha_vencimiento"), "dd-mm-YYYY") & vbTab & rst("comprobante") & vbTab & rst("id_cliente") & vbTab & Mid(rst("ncliente"), 1, 40) & vbTab & rst("descripcion") & vbTab & rst("tc") & vbTab & Format(rst("total"), "#,##0.00") & vbTab & Format(n_saldo_dolar, "#,##0.00") & vbTab & Format(n_saldo, "#,##0.00") & vbTab & in_operador & vbTab & rst("referencia") & vbTab & "[" & rst("factor") & "% ]" & Space(3) & Format(in_tseguro, "###0.00")
             Grilla.AddItem Fila
             
                If IsNull(in_tseguro) = True Then
                    in_factor = 0
                    in_tseguro = 0
                Else
                    in_factor = in_tseguro
                End If
                If rst("id_moneda") = "00002" Then
                  in_factor = in_tseguro * rst("tc")
                Else
                    in_factor = in_tseguro
                End If
                  in_saldo = in_saldo + n_saldo
                  in_saldo_dolar = in_saldo_dolar + n_saldo_dolar
                  nfactor = nfactor + in_tseguro
            If (rst("total") - rst("pago")) > 0 Then
            For k = 8 To 10
                Grilla.col = k
                Grilla.Row = i + 1
                Grilla.CellBackColor = &H80C0FF
            Next k
            End If
            
            If rst("anulado") = "si" Then
           
            For k = 8 To 10
                Grilla.col = k
                Grilla.Row = i + 1
                Grilla.CellBackColor = &H8080FF
            Next k
            End If
            rst.MoveNext
        Next i
        Fila = "" & vbTab & "" & vbTab & "" & vbTab & "" & vbTab & "" & vbTab & "" & vbTab & "" & vbTab & "" & vbTab & Format(in_acumulado_total, "#,##0.00") & vbTab & Format(in_saldo_dolar, "#,##0.00") & vbTab & Format(in_saldo, "#,##0.00") & vbTab & "" & vbTab & "" & vbTab & Format(nfactor, "#,##0.00")
        Grilla.AddItem Fila
   Grilla.ColAlignment(3) = 1
  Grilla.ColAlignment(5) = 1
  Grilla.ColAlignment(7) = 7
  Grilla.ColAlignment(8) = 7
  Grilla.Row = 1
  Grilla.col = 0
  Grilla.ColSel = 1
  Grilla.RowSel = 1
         
Exit Sub
salir: MsgBox "Por Favor Digite Correctamente", vbInformation, "Mensaje para el usuario"

End Sub

Private Sub llenar_recibos(ByVal Grilla As MSHFlexGrid)
