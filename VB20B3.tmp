             Fila = rst("id_movimiento") & vbTab & Format(rst("fecha_emision"), "dd-mm-YYYY") & vbTab & Format(rst("hora"), "HH:mm:ss am/pm") & vbTab & rst("documento") & vbTab & rst("id_cliente") & vbTab & rst("ncliente") & vbTab & Format(rst("total"), "#,##0.00")
