Private Sub load_servicio(ByVal in_servicio As String)

strCadena = "SELECT * FROM view_plan_servicio where id_plan_servicio='" & Val(in_servicio) & "'"
Call ConfiguraRst(strCadena)
If rst.RecordCount > 0 Then
    Me.txtcodigo_plan.Text = rst("id_producto")
