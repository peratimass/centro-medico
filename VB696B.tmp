       Me.chk_certificado.Value = 1
       Me.Dtpcertificado_ini.Value = rst("fecha_cert_inicio")
       Me.Dtpcertificado_fin.Value = rst("fecha_cert_fin")
    Else
       Me.chk_certificado.Value = 0
    End If
    
    If rst("unico_pago") = "si" Then
       Me.chk_unicopago.Value = 1
       If rst("pago_anual") = "si" Then
          Me.chk_pago_anual.Value = 1
       Else
          Me.chk_pago_anual.Value = 0
       End If
    Else
       Me.chk_unicopago.Value = 0
    End If
    
    If rst("pago_mensual") = "si" Then
       Me.chk_pago_mensual.Value = 1
