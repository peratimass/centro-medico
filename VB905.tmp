Public Sub backlog(ByVal Grilla As MSHFlexGrid)
Dim Orden As Integer
Dim espacio As Single

On Error GoTo SALIR
If KEY_USUARIO <> "42546269" Then
    strCadena = "SELECT * FROM view_backlog WHERE dni_colaborador='" & KEY_USUARIO & "' ORDER BY id_codigo ASC "
Else
    strCadena = "SELECT * FROM view_backlog  ORDER BY id_codigo ASC "
End If

Call ConfiguraRstL(strCadena)
If rstL.RecordCount < 1 Then
    Grilla.Rows = 1
    Grilla.Clear
    Exit Sub
End If
   Grilla.Clear
   Grilla.Refresh
   Grilla.Rows = 0
   ReDim arrColWidth(1 To rstL.Fields.Count)
       For Each Campo In rstL.Fields
           Grilla.ColWidth(0) = 900
           Grilla.ColWidth(1) = 4000
           Grilla.ColWidth(2) = 2800
           Grilla.ColWidth(3) = 2500
           Grilla.ColWidth(4) = 1200
           Grilla.ColWidth(5) = 1200
           Grilla.ColWidth(6) = 1200
           Grilla.ColWidth(7) = 1000
           
          Next
         cabecera = "CODIGO" & vbTab & "DESCRIPCION" & vbTab & "PROYECTO" & vbTab & "COLABORADOR" & vbTab & "PRIORIDAD" & vbTab & "FECHA INICIO" & vbTab & "FECHA FIN" & vbTab & "ESTADO"
         Grilla.AddItem cabecera
         For k = 0 To 7
            Grilla.col = k
            Grilla.Row = 0
            Grilla.CellBackColor = &HDFDFE0
         Next k
         
         
        rstL.MoveFirst
       
        For i = 0 To rstL.RecordCount - 1
            Fila = Format(rstL("id_codigo"), "00000") & vbTab & rstL("backlog") & vbTab & rstL("proyecto") & vbTab & rstL("colaborador") & vbTab & rstL("prioridad") & vbTab & Format(rstL("fecha_inicio"), "dd-mm-YYYY") & vbTab & Format(rstL("fecha_final"), "dd-mm-YYYY") & vbTab & rstL("estado")
            Grilla.AddItem Fila
            Fila = ""
            For k = 0 To 7
                Grilla.col = k
                Grilla.Row = i + 1
                Select Case rstL("estado")
                    Case "EN PROCESO"
                            Grilla.CellBackColor = &H80C0FF       ' proceso
                    Case "RECHAZADO"
                            Grilla.CellBackColor = &H8080FF       ' desaprobado
                    Case "APROBADO"
                            Grilla.CellBackColor = &H80FF80    ' aprobado
                    Case "TERMINADO"
                            Grilla.CellBackColor = &HFFFF80       ' terminado
                End Select
            Next k
            rstL.MoveNext
        Next i
  
         
        
         
Exit Sub
SALIR:
Call manejador_error
End Sub
