strCadena = "SELECT id_venta,fecha_emision,documento,total,anulado FROM movimiento_venta WHERE id_cliente='" & dni & "' AND ruc='" & KEY_RUC & "' ORDER BY fecha_emision DESC,numero DESC LIMIT 10"
