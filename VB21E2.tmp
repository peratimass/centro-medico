Dim tpago As Double

strCadena = "SELECT * FROM view_venta_pago WHERE id_venta='" & idVenta & "' and ruc='" & KEY_RUC & "'"
Call ConfiguraRst(strCadena)

If rst.RecordCount < 1 Then
