    strCadena = "SELECT * FROM almacen_producto A,producto P WHERE A.id_producto=P.id_producto AND A.ruc='" & KEY_ALM & "' AND P.ruc='" & KEY_RUC & "' AND A.id_producto='" & Trim(codigoP) & "'"
