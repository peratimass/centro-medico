Private Function put_dni_boleta(ByVal in_dni As String, ByVal in_monto As Single, ByVal in_doc As String) As Boolean

If in_doc <> "0003" Then
    put_dni_boleta = True
    Exit Function
    
End If
If in_doc = "0003" And in_monto < 750 Then
     put_dni_boleta = True
    Exit Function
End If

If in_monto >= 750 And in_doc = "0003" And in_dni <> "00000000" And Len(in_dni) = 8 Then
    put_dni_boleta = True
    Exit Function
Else
    
    MsgBox "Ingrese un DNI VALIDO el monto supera los 750" + Chr(13) + "Configuracion SUNAT", vbInformation, KEY_VENDEDOR
    put_dni_boleta = False
    Exit Function
End If
put_dni_boleta = True


