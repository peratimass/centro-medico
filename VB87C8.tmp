Private Sub TxtNumero_KeyPress(KeyAscii As Integer)
If KeyAscii = 13 Then
    
    
    strCadena = "SELECT `id_compra`,`fecha_emision`,`fecha_cancelacion`,`comprobante`,`id_proveedor`,`nproveedor`,`id_moneda`,`simbolo`,`moneda`,`tc`,`total`,`saldo`,`nombre_completo`,`id_alm`,`ruc`, function_pago_factura(id_compra,'" & Format(Me.DtpFin.Value, "YYYY-mm-dd") & "',id_moneda,ruc) as pago FROM view_cuentas_cobrar WHERE comprobante LIKE '%" & Trim(Me.txtnumero.Text) & "%' and ruc='" & KEY_RUC & "'"
    Call llenarGrid(Me.HfgFacturas)
    
End If
End Sub

Private Sub TxtRuc_KeyPress(KeyAscii As Integer)
If KeyAscii = 13 Then
        
    strCadena = "SELECT `id_compra`,`fecha_emision`,`fecha_cancelacion`,`comprobante`,`id_proveedor`,`nproveedor`,`id_moneda`,`simbolo`,`moneda`,`tc`,`total`,`saldo`,`nombre_completo`,`id_alm`,`ruc`, function_pago_factura(id_compra,'" & Format(Me.DtpFin.Value, "YYYY-mm-dd") & "',id_moneda,ruc) as pago FROM view_cuentas_cobrar WHERE id_proveedor LIKE '%" & Trim(Me.tXTrUC.Text) & "%' and ruc='" & KEY_RUC & "'"
    Call llenarGrid(Me.HfgFacturas)

End If
End Sub
