End If



If Me.DtcTipoDoc.BoundText = "" Or Me.TxtSerie.Text = "" Or Me.TxtNumeroDoc.Text = "" Then
   MsgBox "LLENE TODOS LOS PARAMETROS", vbInformation, KEY_EMPRESA
   Exit Sub
Else
    If Me.DtcAlmacenOrigen.BoundText = KEY_ALM Then
        
        strCadena = "SELECT count(*) FROM movimiento_transferencia WHERE id_doc='" & Me.DtcTipoDoc.BoundText & "' and serie='" & Trim(Me.TxtSerie.Text) & "' and numero='" & Trim(Me.TxtNumeroDoc.Text) & "' and ruc='" & KEY_RUC & "'"
