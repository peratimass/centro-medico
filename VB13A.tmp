    End If
    
    
    Me.TlbGrabar.Buttons(KEY_SAVE).Enabled = False
    Me.TlbGrabar.Buttons(KEY_PRINT).Enabled = True
    StrNumero = FormatosCeros(Trim(Str(Val(Me.TxtNumeroDoc.Text)) + 1), 6)
