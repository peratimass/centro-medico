                MsgBox "Documento ya Ingresado", vbInformation
           Exit Sub
        End If
        
        strCadena = "INSERT INTO movimiento_transferencia(id_doc,dni_atencion,atencion,id_tipo_guia,serie,numero,fecha,direccion,id_destinatario,destinatario,id_transporte,marca_placa,id_chofer,id_alm_origen,id_alm_destino,id_motivo,motivo_otros,observacion,id_venta,dni_save,ruc) " & _
        "VALUES('" & Me.DtcTipoDoc.BoundText & "','" & Trim(Me.txt_dni_atencion.Text) & "','" & Trim(Me.txt_atencion.Text) & "','" & Trim(Me.txt_tipo_transferencia.Text) & "','" & Me.TxtSerie.Text & "','" & Me.TxtNumeroDoc.Text & "','" & KEY_FECHA & "','" & Trim(Me.TxtDireccionDestino.Text) & "','" & Me.TxtRucDestino.Text & "','" & Trim(Me.TxtNombreDestino.Text) & "'," & _
        "'" & Me.TxtRucTransporte.Text & "','" & Trim(Me.TxtMarcayPlaca.Text) & "','" & Me.TxtRucChofer.Text & "','" & Me.DtcAlmacenOrigen.BoundText & "','" & Me.DtcAlmacenDestino.BoundText & "','" & strMotivo & "','" & Trim(Me.TxtOtros.Text) & "','" & Trim(Me.TxtObservacion.Text) & "','" & Val(Me.txtid_venta.Text) & "','" & KEY_USUARIO & "','" & KEY_RUC & "')"
        CnBd.Execute (strCadena)
        Call insertar_acciones(strCadena)
        id_transferencia = LastRegistro("movimiento_transferencia", "id_transferencia")
        If Val(Me.txtid_venta.Text) > 0 Then
            strCadena = "SELECT * FROM movimiento_venta_detalle WHERE id_venta='" & Val(Me.txtid_venta.Text) & "'"
            Call ConfiguraRst(strCadena)
            If rst.RecordCount > 0 Then
               rst.MoveFirst
               For i = 0 To rst.RecordCount - 1
                    strCadena = "INSERT INTO movimiento_transferencia_series(id_transferencia,id_doc,serie,numero,id_producto,chasis,motor,nro_dua,anio_fabricacion,nro_item,ruc)VALUES('" & id_transferencia & "','" & Me.DtcTipoDoc.BoundText & "','" & Trim(Me.TxtSerie.Text) & "','" & Trim(Me.TxtNumeroDoc.Text) & "','" & rst("id_producto") & "','" & rst("nro_chasis") & "','" & rst("serie") & "','" & rst("nro_dua") & "','" & rst("anio_fabricacion") & "','" & rst("nro_item") & "','" & KEY_RUC & "')"
                    CnBd.Execute (strCadena)
                    Call insertar_acciones(strCadena)
                    rst.MoveNext
               Next i
            End If
        Else
            strCadena = "UPDATE movimiento_transferencia_series SET id_transferencia='" & id_transferencia & "' WHERE id_doc='" & Me.DtcTipoDoc.BoundText & "' and serie='" & Trim(Me.TxtSerie.Text) & "' and numero='" & Trim(Me.TxtNumeroDoc.Text) & "' and ruc='" & KEY_RUC & "'"
        CnBd.Execute (strCadena)
        Call insertar_acciones(strCadena)
        End If
        
        
        
        
        
        
        
        If Me.txtLicencia.Text <> "" Then
            strCadena = "UPDATE persona SET licencia='" & Me.txtLicencia.Text & "' WHERE dni='" & Me.TxtRucChofer.Text & "'"
            CnBd.Execute (strCadena)
            Call insertar_acciones(strCadena)
        End If
    Else
         If verficar_series_recibidas(Val(Me.TxtId_transferencia.Text)) = False Then
             Exit Sub
         End If
         strCadena = "UPDATE movimiento_transferencia SET finalizado='si',id_recibio='" & KEY_USUARIO & "',nsave='" & KEY_VENDEDOR & "' WHERE id_transferencia='" & Val(Me.TxtId_transferencia.Text) & "' ANd ruc='" & KEY_RUC & "'"
