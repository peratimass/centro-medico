        End If
        Select Case rst("id_motivo")
            Case 1
                Me.OpVenta.Value = True
            Case 2
                Me.OptDevolucion.Value = True
            Case 3
                Me.OptGrinter.Value = True
            Case 4
                Me.OptOtros.Value = True
